{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Broccoli",
  "description": "A simple, fast, and reliable job queue for Rust",
  "colors": {
    "primary": "#4A9F4A",
    "light": "#6BC96B",
    "dark": "#3D8B3D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "installation",
              "quickstart"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/producers",
              "guides/consumers",
              "guides/retry-strategies",
              "guides/message-scheduling"
            ]
          },
          {
            "group": "Brokers",
            "pages": [
              "brokers/redis",
              "brokers/rabbitmq",
              "brokers/surrealdb"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Core API",
            "pages": [
              "api-reference/broccoli-queue",
              "api-reference/broker-message",
              "api-reference/publish-options",
              "api-reference/consume-options",
              "api-reference/retry-strategy",
              "api-reference/errors"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Docs",
          "href": "https://docs.rs/broccoli_queue",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Crates.io",
          "href": "https://crates.io/crates/broccoli_queue",
          "icon": "box"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Docs.rs",
        "href": "https://docs.rs/broccoli_queue"
      }
    ],
    "primary": {
      "type": "github",
      "href": "https://github.com/densumesh/broccoli"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/densumesh/broccoli"
    }
  }
}
